<?xml version="1.0"?>
<launch>
  <arg name="robot" default="swb"/>

  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
	
  <node
    name="tf_base_map"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 base_link map 100" />
	
	<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find swb_description)/urdf/swb_description.urdf.xacro'"/>

  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" 
    args="-param robot_description -urdf -z 0.4 -x 0 -y 0 -Y 0 -model $(arg robot)" /> 
  
</launch>